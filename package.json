{
  "name": "webpack5_boilerplate",
  "version": "1.0.0",
  "description": "Webpack React Typescript setup",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development SERVER=true webpack serve",
    "build": "cross-env NODE_ENV=production webpack",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:analyze": "cross-env NODE_ENV=production ANALYZER=true webpack",
    "prettier": "prettier src/**/*.{ts,tsx,css,scss}",
    "lint:prettier": "npm run prettier --check",
    "lint:prettier:fix": "npm run prettier --write",
    "lint:es": "eslint src --ext .js,.ts,.tsx",
    "lint:es:fix": "npm run lint:es -- --fix",
    "lint:all:fix": "npm run lint:es:fix && npm run lint:prettier:fix"
  },
  "keywords": [
    "webpack",
    "typescript",
    "react",
    "eslint",
    "postcss",
    "prettier"
  ],
  "author": "https://github.com/Bysees",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-css-import-order": "^1.1.0",
    "eslint-webpack-plugin": "^3.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "sass": "^1.56.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.8.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
